{
	"id": "required_content_fields",
	"fields": [
		{
			"name": "state",
			"id": "state",
			"required":true,
			"type": "Select",
			"label": "Content State",
			"default":1,
			"select_options":[
				{"value":1,"text":"Published"},
				{"value":0,"text":"Unpublished"}
			],
			"coltype":"tinyint NOT NULL DEFAULT '1'",
			"filter":{
				"IntVal": []
			}
		},
		{
			"label":"Source",
			"name":"old_url",
			"id":"old_url",
			"type":"Text",
			"maxlength":4096,
			"description":"",
			"required":true,
			"wrapclass":"bigtext",
			"nowrap":false,
			"filter":{
				"StringVal": [],
				"Length": [0, 4096]
			}
		},
		{
			"label":"Destination",
			"name":"new_url",
			"id":"new_url",
			"type":"Text",
			"maxlength":4096,
			"description":"",
			"required":true,
			"wrapclass":"bigtext",
			"nowrap":false,
			"filter":{
				"StringVal": [],
				"Length": [0, 4096]
			}
		},
		{
			"type":"Select",
			"id":"header",
			"name":"header",
			"required":true, 
			"label":"Header/Status Code",
			"default":"301",
			"description":"Redirect header type - usually 301.",
			"placeholder":"Please select",
			"select_options":[
				{"text":"301 - Permanent","value":"301"},
				{"text":"302 - Temporary","value":"302"}
			],
			"filter":{
				"IntVal": [],
				"Between": [300, 303]
			}
		},
		{
			"label":"Note",
			"name":"note",
			"id":"note",
			"type":"Text",
			"required":false,
			"maxlength":255,
			"description":"Not required, but may be useful.",
			"default":"",
			"coltype":"varchar (255)",
			"filter":{
				"StringVal": [],
				"Length": [0, 255]
			}
		}
		
	]
}

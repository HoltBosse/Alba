{
	"id":"testform",
	"fields":[
		{
			"type":"Select",
			"id":"logic1",
			"name":"logic1",
			"required":true, 
			"label":"Content Type",
			"default":"id",
			"description":"Set teaser ordering option.",
			"filter":"STRING",
			"placeholder":"Please select",
			"select_options":[
				{"text":"Lesson","value":"lesson"},
				{"text":"PDF","value":"pdf"},
				{"text":"Video","value":"video"}
			]
		},
		{
			"type":"Checkbox",
			"id":"logic3",
			"name":"logic3",
			"label":"Double PDF?",
			"logic":[
				[
					{
						"field": "logic1",
						"test": "==",
						"value": "pdf"
					}
				]
			],
			"default":0
		},
		{
			"type":"Text",
			"name":"logic2",
			"id":"logic2",
			"required":true,
			"label":"Show if PDF",
			"logic":[
				[
					{
						"field": "logic1",
						"test": "==",
						"value": "pdf"
					}
				]
			]
		},
		{
			"type":"Text",
			"name":"logic4",
			"id":"logic4",
			"required":true,
			"label":"Show if checkbox checked",
			"logic":[
				[
					{
						"field": "logic3",
						"test": "==",
						"value": "1"
					},
					{
						"field": "logic1",
						"test": "==",
						"value": "pdf"
					}
				]
			]
		},
		{
			"name":"submit",
			"id":"submit",
			"type":"HTML",
			"html":"<div><button type='submit btn'>Submit</button></div>"
		}
	]
}